{
    "name": "roberts/work-comp-leads",
    "description": "Laravel package for Workers Compensation Insurance",
    "keywords": [
        "roberts",
        "work-comp-leads"
    ],
    "homepage": "https://github.com/roberts/work-comp-leads",
    "license": "MIT",
    "authors": [
        {
            "name": "Drew Roberts",
            "email": "packages@drewroberts.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "livewire/livewire": "^2.4",
        "tipoff/authorization": "^2.5.5",
        "tipoff/support": "^1.6.3"
    },
    "require-dev": {
        "tipoff/test-support": "^1.2.0"
    },
    "autoload": {
        "psr-4": {
            "Roberts\\WorkCompLeads\\": "src",
            "Roberts\\WorkCompLeads\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Roberts\\WorkCompLeads\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Roberts\\WorkCompLeads\\WorkCompLeadsServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
