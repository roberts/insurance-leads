{
    "name": "roberts/leads",
    "description": "Laravel package for Workers Compensation Insurance",
    "keywords": [
        "roberts",
        "leads"
    ],
    "homepage": "https://github.com/roberts/leads",
    "license": "MIT",
    "authors": [
        {
            "name": "Drew Roberts",
            "email": "packages@drewroberts.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "bensampo/laravel-enum": "^3.3",
        "tipoff/authorization": "^2.8",
        "tipoff/locations": "^2.9.8",
        "tipoff/statuses": "^2.2",
        "tipoff/support": "^2.0.2"
    },
    "require-dev": {
        "tipoff/test-support": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Roberts\\Leads\\": "src",
            "Roberts\\Leads\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Roberts\\Leads\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Roberts\\Leads\\LeadsServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
